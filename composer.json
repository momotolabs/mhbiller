{
  "name": "momotolabs/mhbiller",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Momotolabs\\Mhbiller\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Momotolabs\\MhBiller\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "José Chavarría",
      "email": "jchavarria@momotolabs.com"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "laravel/pint": "^1.13",
    "orchestra/testbench": "v8.22.1",
    "pestphp/pest": "^2.30",
    "phpstan/phpstan": "^1.10"
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\Momotolabs\\MhBiller\\Providers\\PackageServiceProvider"
      ],
      "aliases": {
        "MhBiller": "Momotolabs\\MhBiller\\MhBiller"
      }
    }
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "ergebnis/json-schema-validator": "^4.2",
    "justinrainbow/json-schema": "^5.2",
    "opis/json-schema": "^2.3",
    "swaggest/json-schema": "^0.12.42",
    "vlucas/phpdotenv": "^5.6"
  }
}
